{
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "importOrder": [
    "^fastify$",
    "fastify",
    "mercurius",
    "^[^.]+$",
    "^\\./utils",
    "^\\./entity",
    "^\\./modules",
    "^\\./"
  ],
  "importOrderSeparation": true
}